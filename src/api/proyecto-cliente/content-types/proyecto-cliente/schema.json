        "desarrollo web",
        "redisenio web",
        "tienda online",
        "academia web",
        "aplicacion web",
        "marketing digital",
        "posisionamiento seo",
        "rendimiento web",
        "disenio marca",
        "servicio varios",
        "otro"
      ],
      "default": "desarrollo web",
      "required": true
    },
    "descripcion": {
      "type": "text",
      "required": false
    },
    "politica_privacidad": {
      "type": "boolean",
      "default": false
